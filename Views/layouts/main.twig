<!doctype html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7" lang=""> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8" lang=""> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9" lang=""> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js" lang=""> <!--<![endif]-->
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="{% block meta_description %}MADISON{% endblock %}">
    <meta name="keywords" content="{% block meta_keywords %}MADISON{% endblock %}">
    {% include 'partials/head.twig' %}
    <title>{% block title %}MADISON{% endblock %}</title>
</head>
<body>
    {% include 'partials/leftpanel.twig' %}

    <div id="right-panel" class="right-panel">
        {% include 'partials/header.twig' %}
        <div class="content mt-3">
            <div class="breadcrumbs">
                <div class="col-sm-4">
                    <div class="page-header float-left">
                        <div class="page-title">
                            {% block pageTitle %}Home{% endblock %}
                        </div>
                    </div>
                </div>
                <div class="col-sm-8">
                    <div class="page-header float-right">
                        <div class="page-title">
                            <ol class="breadcrumb text-right">
                                {% block breadcrumbs %}
                                    <li class="active">Madison</li>
                                {% endblock %}
                            </ol>
                        </div>
                    </div>
                </div>
            </div>
            {% block content %}{% endblock %}
        </div>
    </div>

    {% include 'partials/footer.twig' %}

    {% block scripts %}{% endblock %}
</body>
</html>
